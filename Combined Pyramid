// Q3. Combined Pyramid
import java.util.*;
public class Q3Combined_Pyramid {
    public static void main(String args[]){
        int rows;
        Scanner sc = new Scanner(System.in);
        rows = sc.nextInt();
        for(int i=1; i<=rows; ++i){
            for(int j=1; j<=i; ++j){
                System.out.println(j + "*");
            }
        }
}
}